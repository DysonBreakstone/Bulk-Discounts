<%= form_with model: model, url: url, method: method, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %><br/>
  <%= form.label :threshold %>
  <%= form.number_field :threshold %><br/>
  <%= form.label :discount %>
  <%= form.number_field :discount %><br/>
  <%= form.submit %>
<% end %>